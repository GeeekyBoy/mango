{
  "compilerOptions": {
    "target": "ESNext",
    "module": "Node16",
    "allowJs": true,
    "checkJs": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "maxNodeModuleJsDepth": 2,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noUnusedLocals": true
  }
}