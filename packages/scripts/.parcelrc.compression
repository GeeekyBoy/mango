{
  "extends": "./.parcelrc",
  "transformers": {
    "*.{css,pcss}": [
      "@parcel/transformer-postcss",
      "@mango-js/parcel-transformer-css",
      "@parcel/transformer-inline-string"
    ],
    "htmlStyleSheet:*.{css,pcss}": [
      "@parcel/transformer-postcss",
      "@mango-js/parcel-transformer-css"
    ]
  },
  "compressors": {
    "*.{html,css,js,svg,map}": [
      "...",
      "@parcel/compressor-gzip",
      "@parcel/compressor-brotli"
    ]
  }
}
