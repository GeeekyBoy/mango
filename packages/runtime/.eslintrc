{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "env": {
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 3
  },
  "globals": {
    "ActiveXObject": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:jsdoc/recommended"
  ],
  "rules": {
    "jsdoc/tag-lines": 0,
    "no-cond-assign": 0
  }
}
